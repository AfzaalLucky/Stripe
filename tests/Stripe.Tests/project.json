  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    },
    "NETStandard.Library": "1.6.1",
    "NUnitLite": "3.5.0",
    "Stripe" : "1.0.*",
    "ServiceStack.Interfaces" : "1.0.*",
    "ServiceStack.Text" : "1.0.*",
    "ServiceStack.Client" : "1.0.*"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "imports": "dnxcore50",
      "dependencies": {
        "System.Reflection.TypeExtensions": "4.*"
      }
    }
  },
  "version": "1.0.0"
